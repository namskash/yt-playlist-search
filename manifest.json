{
  "manifest_version": 3,
  "name": "YouTube Playlist Search",
  "version": "1.0",
  "description": "Search feature for your YouTube playlists!",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "css": ["styles.css"],
      "matches": ["https://www.youtube.com/*"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
